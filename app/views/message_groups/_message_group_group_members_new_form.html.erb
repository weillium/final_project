<%= form_with(model: @group_member, local: true) do |f| %>

  <%= f.hidden_field :group_id, value: @message_group.id %>

  <div class="form-group">
    <%= f.label :user_id %>

    <%= f.collection_select :user_id, User.all, :id, :to_s, {}, class: "form-control" %>
  </div>

  <%= f.submit "Create group member", class: "btn btn-block btn-outline-secondary" %>
<% end %>
